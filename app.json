{
  "expo": {
    "name": "Cumple-app",
    "slug": "Cumple-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/regalo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/regalo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juan123456789.Cumpleapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/regalo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "12dd324e-7d91-40a7-a699-76717cac674d"
      }
    }
  }
}
